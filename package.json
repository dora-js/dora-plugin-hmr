    "babel-plugin-add-module-exports": "~0.1.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "dora": "0.x",
    "dora-plugin-webpack": "0.x",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "expect": "^1.16.0",
    "mocha": "^2.4.5",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "supertest": "^1.1.0"
  },
  "peerDependencies": {
    "atool-build": "0.x",
    "dora-plugin-webpack": "0.x",
    "dora": "0.4.x"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
